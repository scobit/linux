# install
apt install vim

# go to insert mode
i

# go to command mode
esc

# write changes and exit
:wq

# quit disgarding changes
:q!

# delete current line
dd

# delete 10 lines from curent
d 10 d

# undo changes
u

# start edit from next characte
a

# start editing at the end of line
A

# go to start of line
0

# go to the end of line
$

# go to 12 line
12G

# search in document
/pattern 
then n or N

# substitude values
:%s/old/new
